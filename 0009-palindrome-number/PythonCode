class Solution(object):
    def isPalindrome(self, x):
        """
        :type x: int
        :rtype: bool
        """
        """ 
        Method 1

        org=x
        temp=0
        while(x>0):
            rem=x%10
            temp=temp*10+rem
            x//=10
        if(temp==org):
            return True
        return False """

        # Method 2

        value=str(x)[::-1]
        org=str(x)
        return value==org
        

